"use strict";(function(a){jQuery(document).on("ready",function(){if(document.getElementsByClassName("fullpage")){a("#fullpage").fullpage({menu:"#mneu2, #menu",autoScrolling:true,scrollHorizontally:true,anchors:["home","about","portfolio","testimonials","blog","contact"],responsiveWidth:1199.98,responsiveHeight:600,lockAnchors:false,css3:true})}a('[data-toggle="tooltip"]').tooltip();tippy.setDefaults({arrow:true,delay:40,placement:"right",animation:"perspective",theme:"light"});a(".menu-click").on("click",function(){a(".main-menu > ul").toggleClass("show");return false});a(".menu-icon").on("click",function(){a(".menu-icon").toggleClass("open-menu");a(".menu-box").toggleClass("active")});a(".hikmaclose-menu").on("click",function(){a(".menu-icon").removeClass("open-menu");a(".menu-box").removeClass("active")});a(".has-child > a, .menu-item-has-children > a").on("click",function(){var p=a(this).parent("li");if(p.hasClass("open")){p.removeClass("open");p.find("li").removeClass("open");p.find("ul").slideUp(500,"linear")}else{p.addClass("open");p.children("ul").slideDown();p.siblings("li").children("ul").slideUp();p.siblings("li").removeClass("open");p.siblings("li").find("li").removeClass("open");p.siblings("li").find("ul").slideUp()}});a("#mneu34 li > a, #sidemenuscoll ul li > a, .main-menu ul li > a").on("click",function(q){var p=a(this);a("html, body").stop().animate({scrollTop:a(p.attr("href")).offset().top-80},1200,"easeInOutExpo");q.preventDefault()});var d=function d(){return a(window).width()};var c=function c(){return a(window).height()};var b=function b(p){a(p).width(d()).height(c())};var l=function l(p){if(typeof window.matchMedia!=="undefined"){var q=window.matchMedia("(max-width:"+p+"px)");if(q.matches){return true}else{return false}}else{if(d()<=p){return true}else{return false}}};a(window).on("load",function(){a(".animated-row").each(function(){var p=a(this);p.find(".animate").each(function(t){var q=a(this);var r=q.data("animate");var s=q.attr("data-animation-delay");q.css({"-webkit-animation-delay":s,"-moz-animation-delay":s,"animation-delay":s});q.on("inview",function(u,v){if(v){setTimeout(function(){q.addClass("animated "+r).removeClass("animate")},t*50)}else{if(!l(767)){q.removeClass("animated "+r).addClass("animate")}}})})})});a(".js-tilt").tilt({glare:true,maxGlare:0.5});a(".grid").imagesLoaded(function(){var p=a(".grid").isotope({itemSelector:".grid-item",percentPosition:true,masonry:{columnWidth:0}})});a(".filter-menu").on("click","li",function(){a(".filter-menu li").removeClass("active");a(this).addClass("active");var p=a(this).attr("data-filter");a(".grid").isotope({filter:p});a(window).trigger("resize")});a(".hikmatestimonial-active").owlCarousel({loop:true,margin:10,mouseDrag:true,responsive:{210:{items:1},320:{items:1},479:{items:1},768:{items:1},980:{items:1},1199:{items:1}}});a(".feature-blog-slideractive").owlCarousel({loop:true,margin:0,autoplay:true,nav:true,navText:['<i class="icofont-caret-left"></i>','<i class="icofont-caret-right"></i>'],autoplayTimeout:4000,responsive:{210:{items:1},320:{items:1},479:{items:1},768:{items:1},980:{items:1},1199:{items:1}}});a(".menu-box").overlayScrollbars({className:"os-theme-round-light"});if(document.getElementById("instragramslider_active")){var g=document.getElementById("instragramslider_active");var i=g.getAttribute("data-instra-id");var h=g.getAttribute("data-instra-token");var f=new Instafeed({get:"user",userId:i,accessToken:h,sortBy:"most-liked",template:'<a class="instra-item" href="{{link}}" target="_blank"><img  src="{{image}}" /> <div class="instra-hover"><i class="icofont-link"></i></div></a>',target:g,limit:9,resolution:"low_resolution",after:function e(){a(g).owlCarousel({loop:true,margin:0,nav:false,autoplay:true,autoplayTimeout:3000,responsive:{210:{items:1},320:{items:1},479:{items:1},768:{items:1},980:{items:1},1199:{items:1}}})}});f.run()}a(window).on("scroll",function(){var p=a(window).scrollTop();if(p<200){a(".site-header.sticky-header .navbar").removeClass("sticky fadeInDown animated")}else{a(".site-header.sticky-header .navbar").addClass("sticky fadeInDown animated")}});a(".main-menu ul li").on("click",function(){a(".main-menu ul li").removeClass("active");a(this).addClass("active")});var j,n=a(".main-menu"),o=n.outerHeight()+15,k=n.find("a"),m=k.map(function(){var p=a(a(this).attr("href"));if(p.length){return p}});k.on("click",function(p){var q=a(this).attr("href"),r=q==="#"?0:a(q).offset().top-o+1;a("html, body").stop().animate({scrollTop:r},300);p.preventDefault()});a(window).on("scroll",function(){var q=a(this).scrollTop()+o;var p=m.map(function(){if(a(this).offset().top<q){return this}});p=p[p.length-1];var r=p&&p.length?p[0].id:"";if(j!==r){j=r;k.parent().removeClass("active").end().filter("[href='#"+r+"']").parent().addClass("active")}});jQuery(window).load(function(){jQuery(".hikma-preloder").fadeOut(300)});a.scrollUp({scrollText:'<i class="fa fa-angle-up"></i>'});a(".img-popup").magnificPopup({type:"image",gallery:{enabled:true}})})})(jQuery);